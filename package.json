{
  "name": "syonet_eight",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "dev:www": "cd projects/syonet_eight_www && next dev",
    "build:www": "cd projects/syonet_eight_www && next build",
    "start:www": "cd projects/syonet_eight_www && next start",
    "lint:www": "cd projects/syonet_eight_www && next lint",
    "test:www": "cd projects/syonet_eight_www && cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "deploy:www": "cd projects/syonet_eight_www && cross-env NODE_ENV=production SLS_DEBUG=* serverless",
    "destroy:www": "cd projects/syonet_eight_www && cross-env NODE_ENV=production serverless remove --force",
    "dev:design_system": "cd projects/syonet_eight_design_system && tsc -w",
    "build:design_system": "cd projects/syonet_eight_design_system && tsc",
    "lint:design_system": "cd projects/syonet_eight_design_system && next lint",
    "test:design_system": "cd projects/syonet_eight_design_system &&  cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "dev": "npm-run-all --parallel dev:www dev:design_system",
    "build": "npm-run-all --parallel build:www build:design_system",
    "start": "npm-run-all --parallel start:www",
    "lint": "npm-run-all --parallel lint:www lint:design_system",
    "test": "npm-run-all --parallel test:www test:design_system",
    "rm": "rimraf",
    "rm:all": "rimraf projects/syonet_eight_design_system/dist projects/syonet_eight_www/.next node_modules",
    "yarn:clean": "yarn cache clean design_system",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "deploy:storybook": "cd projects/syonet_eight_storybook && cross-env SLS_DEBUG=* serverless deploy",
    "destroy:storybook": "cd projects/syonet_eight_storybook && serverless remove --force",
    "storycap": "storycap --serverCmd \"npx http-server storybook-static -p 9001\" http://localhost:9001",
    "vrt": "reg-suit run"
  },
  "dependencies": {
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "design_system": "file:./projects/syonet_eight_design_system"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/react": "6.3.12",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.9",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "@types/react-test-renderer": "17.0.1",
    "babel-loader": "8.2.3",
    "cross-env": "7.0.3",
    "esbuild": "0.13.15",
    "esbuild-jest": "0.5.0",
    "eslint": "7.23.0",
    "eslint-config-next": "12.0.4",
    "jest": "27.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "puppeteer": "11.0.0",
    "reg-keygen-git-hash-plugin": "0.11.1",
    "reg-notify-github-plugin": "0.11.1",
    "reg-publish-s3-plugin": "0.11.0",
    "reg-suit": "0.11.1",
    "rimraf": "3.0.2",
    "serverless": "2.66.2",
    "storybook-css-modules-preset": "1.1.1",
    "storycap": "3.1.0",
    "typescript": "4.5.2"
  },
  "resolutions": {
    "@types/react": "17.0.35"
  },
  "workspaces": [
    "projects/syonet_eight_www",
    "projects/syonet_eight_design_system"
  ]
}
